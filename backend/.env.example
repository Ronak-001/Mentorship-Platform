PORT=5000

# MongoDB Connection String
# Option 1: Local MongoDB (if MongoDB is installed locally)
# MONGODB_URI=mongodb://localhost:27017/mentorconnect

# Option 2: MongoDB Atlas (Recommended - Free tier available)
# Get connection string from: https://www.mongodb.com/cloud/atlas
# Format: mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/mentorconnect?retryWrites=true&w=majority
MONGODB_URI=your_mongodb_connection_string_here

JWT_SECRET=your_jwt_secret_key_here
NODE_ENV=development
